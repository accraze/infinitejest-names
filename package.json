{
  "name": "infinitejest-names",
  "description": "retrieve a random name from DFW's monolithic novel Infinite Jest",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accraze/infinitejest-names.git"
  },
  "keywords": [
    "random",
    "infinite",
    "jest",
    "dfw",
    "david",
    "foster",
    "wallace"
  ],
  "author": "accraze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/accraze/infinitejest-names/issues"
  },
  "homepage": "https://github.com/accraze/infinitejest-names",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.3.0",
    "codecov.io": "0.1.6",
    "commitizen": "1.0.4",
    "cz-conventional-changelog": "1.1.0",
    "ghooks": "0.3.2",
    "istanbul": "0.3.20",
    "mocha": "2.3.3",
    "semantic-release": "^4.3.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
